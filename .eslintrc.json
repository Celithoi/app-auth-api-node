{
  "env": {
    "es2021": true,
    "node": true,
    "commonjs": true
  },
  "extends": [
    "airbnb-base",
    "plugin:prettier/recommended"
  ],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "prettier/prettier": "error",
    "no-console": "warn",
    "class-methods-use-this": "off",
    "consistent-return": "off",
    "no-unused-vars": ["warn", { "argsIgnorePattern": "^_" }]
  }
}